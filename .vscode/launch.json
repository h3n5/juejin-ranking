{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "My Nuxt project",
      "protocol": "inspector",
      "program": "${workspaceRoot}/server/index.js",
      "stopOnEntry": true,
      "args": ["dev"],
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "nuxt:*,app,client"
      }
    }
  ],
  "compounds": []
}
